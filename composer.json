{
    "name": "esign/inline-editing",
    "description": "use inline editing for text and translations",
    "keywords": [
        "esign",
        "laravel",
        "inline",
        "editing"
    ],
    "homepage": "https://github.com/Esign/InlineEditing",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tijl CLottemans",
            "email": "tijlclottemans@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\InlineEditing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\InlineEditing\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\InlineEditing\\InlineEditingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
